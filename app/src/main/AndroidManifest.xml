<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="cn.edu.gdmec.android.mobileguard">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"

        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="cn.edu.gdmec.android.mobileguard.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="cn.edu.gdmec.android.mobileguard.m1home.HomeActivity"/>
        <activity android:name="cn.edu.gdmec.android.mobileguard.m2theftguard.LostFindActivity"/>
        <activity android:name="cn.edu.gdmec.android.mobileguard.m2theftguard.Setup1Activity"/>
        <activity android:name="cn.edu.gdmec.android.mobileguard.m2theftguard.Setup2Activity"/>
        <activity android:name="cn.edu.gdmec.android.mobileguard.m2theftguard.Setup3Activity"/>
        <activity android:name="cn.edu.gdmec.android.mobileguard.m2theftguard.Setup4Activity"/>


    </application>




</manifest>